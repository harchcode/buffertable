{
  "name": "buffertable-node",
  "version": "0.0.2",
  "description": "Lightweight table of buffers that can be sent to web server or browser",
  "repository": {
    "type": "git",
    "url": "https://github.com/harchcode/buffertable",
    "directory": "buffertable-node"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "ava './test/**/*ts' --verbose",
    "lint": "eslint './{src,test}/**/*.{js,ts}'",
    "benchmark": "ts-node benchmark/simple.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "ava": "^3.5.0",
    "benchmark": "^2.1.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}
